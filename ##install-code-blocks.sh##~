echo "hi" >>  /etc/apt/sources.list
echo '"deb [arch=amd64,i386] https://apt.jenslody.de/stable jessie main"' >> /etc/apt/sources.list
echo "deb-src https://apt.jenslody.de/stable jessie main" >> /etc/apt/sources.list
apt-get -y install git
apt-get -y update
apt-get -y -f install
apt-get -y install apt-transport-https
aptitude -y update
aptitude -y install build-essential
apt-get -y install gdb

